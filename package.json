{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "yarn lint:scripts & yarn lint:styles",
        "lint:scripts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:styles": "stylelint \"**/*.{css,scss}\"",
        "preview": "vite preview",
        "prepare": "husky",
        "commitlint": "commitlint --edit",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
        "clean": "yarn clean:dependencies & yarn clean:builds & yarn clean:hidden-builds",
        "clean:dependencies": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
        "clean:hidden-builds": "find . -name \".dist\" -type d -prune -exec rm -rf '{}' +",
        "clean:builds": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
        "clean:generated": "rm -rf ./src/shared/api/generated"
    },
    "dependencies": {
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.39.1",
        "@tanstack/router-plugin": "^1.136.1",
        "@types/node": "^24.10.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "@vitejs/plugin-react": "^5.1.0",
        "babel-plugin-react-compiler": "^1.0.0",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-file-progress": "^3.0.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^62.0.0",
        "eslint-plugin-unused-imports": "^4.3.0",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.6",
        "pinst": "^3.0.0",
        "stylelint": "^16.25.0",
        "stylelint-config-idiomatic-order": "^10.0.0",
        "stylelint-config-prettier-scss": "^1.0.0",
        "stylelint-config-standard-scss": "^16.0.0",
        "stylelint-order": "^7.0.0",
        "stylelint-prettier": "^5.0.3",
        "stylelint-scss": "^6.12.1",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.3",
        "vite": "npm:rolldown-vite@7.2.2"
    },
    "resolutions": {
        "vite": "npm:rolldown-vite@7.2.2"
    },
    "packageManager": "yarn@4.3.1",
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "eslint"
        ],
        "**/*.scss": [
            "stylelint"
        ]
    }
}
