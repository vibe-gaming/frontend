{
    "name": "frontend",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "yarn generate:api && vite --port 3000 --host && dotenv -e .env.development.local",
        "build": "yarn generate:api && vite build && tsc",
        "lint": "yarn lint:scripts & yarn lint:styles",
        "lint:scripts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:styles": "stylelint \"**/*.{css,scss}\"",
        "generate:cert": "mkdir -p .cert && mkcert -key-file ./.cert/key.pem -cert-file ./.cert/cert.pem 'localhost'",
        "generate:api": "kubb --config ./kubb.config.ts",
        "preview": "vite preview",
        "prepare": "husky",
        "commitlint": "commitlint --edit",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
        "clean": "yarn clean:dependencies & yarn clean:builds & yarn clean:hidden-builds",
        "clean:dependencies": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' +",
        "clean:hidden-builds": "find . -name \".dist\" -type d -prune -exec rm -rf '{}' +",
        "clean:builds": "find . -name \"dist\" -type d -prune -exec rm -rf '{}' +",
        "clean:generated": "rm -rf ./src/shared/api/generated",
        "cspell": "yarn cspell src/**/*.{ts,tsx,css,scss} --config .cspell.json --words-only --unique"
    },
    "dependencies": {
        "@blasdfaa/bvi": "^1.0.0",
        "@chakra-ui/react": "^3.29.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/inter": "^5.2.8",
        "@tanstack/react-query": "^5.90.9",
        "@tanstack/react-router": "^1.136.1",
        "@tanstack/react-router-devtools": "^1.136.1",
        "@types/lodash.isequal": "^4.5.8",
        "@use-gesture/react": "^10.3.1",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "autoprefixer": "^10.4.22",
        "axios": "^1.13.2",
        "browserslist": "^4.28.0",
        "browserslist-to-esbuild": "^2.1.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.19",
        "framer-motion": "^12.23.24",
        "is-inn-js": "^1.1.0",
        "jotai": "^2.15.1",
        "lodash.isequal": "^4.5.0",
        "lucide-react": "^0.553.0",
        "next-themes": "^0.4.6",
        "postcss": "^8.5.6",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.66.1",
        "react-icons": "^5.5.0",
        "sass": "^1.94.0",
        "sonner": "^2.0.7",
        "use-debounce": "^10.0.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "@cspell/dict-bash": "^4.2.2",
        "@cspell/dict-css": "^4.0.18",
        "@cspell/dict-git": "^3.0.7",
        "@cspell/dict-html": "^4.0.12",
        "@cspell/dict-html-symbol-entities": "^4.0.4",
        "@cspell/dict-markdown": "^2.0.12",
        "@cspell/dict-ru_ru": "^2.3.2",
        "@cspell/dict-typescript": "^3.2.3",
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.39.1",
        "@kubb/cli": "^4.5.9",
        "@kubb/core": "^4.5.9",
        "@kubb/plugin-client": "^4.5.9",
        "@kubb/plugin-oas": "^4.5.9",
        "@kubb/plugin-react-query": "^4.5.9",
        "@kubb/plugin-ts": "^4.5.9",
        "@kubb/plugin-zod": "^4.5.9",
        "@kubb/react": "^4.3.1",
        "@tanstack/react-query-devtools": "^5.90.2",
        "@tanstack/router-core": "^1.139.0",
        "@tanstack/router-plugin": "^1.136.1",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.46.4",
        "@typescript-eslint/parser": "^8.46.4",
        "@vitejs/plugin-react": "^5.1.0",
        "babel-plugin-react-compiler": "^1.0.0",
        "cspell": "^9.3.1",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-file-progress": "^3.0.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^62.0.0",
        "eslint-plugin-unused-imports": "^4.3.0",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.6",
        "pinst": "^3.0.0",
        "prettier": "^3.6.2",
        "sharp": "^0.34.5",
        "stylelint": "^16.25.0",
        "stylelint-config-idiomatic-order": "^10.0.0",
        "stylelint-config-prettier-scss": "^1.0.0",
        "stylelint-config-standard-scss": "^16.0.0",
        "stylelint-order": "^7.0.0",
        "stylelint-prettier": "^5.0.3",
        "stylelint-scss": "^6.12.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3",
        "vite": "npm:rolldown-vite@7.2.2",
        "vite-bundle-analyzer": "^1.2.3",
        "vite-plugin-pwa": "^1.1.0",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "packageManager": "yarn@4.9.1",
    "resolutions": {
        "vite": "npm:rolldown-vite@7.2.2"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "eslint"
        ],
        "**/*.scss": [
            "stylelint"
        ]
    }
}
